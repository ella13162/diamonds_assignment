<!DOCTYPE html>
<html lang="en">
<head>
   <title>Basket</title>
   <%-include('layouts/header'); -%>

</head>
<body>
   <h1>Basket Page</h1>
   <ul>
       <% basketItems.forEach(item => { %>
           <li>
               Product: <%= item.product.product_id %>,
               Quantity: <%= item.quantity %>
           </li>
       <% }); %>
   </ul>
   <%- include('layouts/footer'); -%>
   <%- include('layouts/scripts'); -%>
</body>
</html>
